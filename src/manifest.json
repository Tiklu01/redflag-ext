{
  "manifest_version": 3,
  "name": "Resume ATS Checker",
  "version": "1.0",
  "description": "A fun ATS checker that interprets resumes sarcastically.",
  "action": {
    "default_popup": "https://redflag-ext-gqaq.vercel.app/popup", 
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },

  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
